{
    "name": "ng2-n3-test",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "doc": "yarn compodoc --tsconfig tsconfig.base.json --theme Readthedocs --output dist/ng2-n3-test/compodoc --disableRoutesGraph",
        "serve:doc": "yarn doc --serve --watch",
        "build:doc": "yarn doc",
        "build": "ng build",
        "build:app-shell": "ng run ng2-n3-test:app-shell:production",
        "prebuild:prod": "yarn build:app-shell",
        "build:prod": "yarn build --prod",
        "postbuild:prod": "run-s gzip:dist build:doc",
        "build:gh": "yarn build:prod --base-href=/ng2-n3-test/",
        "gzip:dist": "gzipper compress ./dist/ng2-n3-test/",
        "test": "ng test",
        "test:cov": "ng test --code-coverage true",
        "test:ci": "yarn test:cov --configuration=ci",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "deploy:ci": "ng deploy --no-build --message=\"Release $npm_package_name (v$npm_package_version) on gh-pages\""
    },
    "prettier": {
        "printWidth": 120,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": true,
        "jsxSingleQuote": true,
        "trailingComma": "none",
        "bracketSpacing": true,
        "jsxBracketSameLine": true,
        "arrowParens": "avoid"
    },
    "dependencies": {
        "@angular/animations": "~10.0.4",
        "@angular/common": "~10.0.4",
        "@angular/compiler": "~10.0.4",
        "@angular/core": "~10.0.4",
        "@angular/forms": "~10.0.4",
        "@angular/platform-browser": "~10.0.4",
        "@angular/platform-browser-dynamic": "~10.0.4",
        "@angular/platform-server": "~10.0.4",
        "@angular/router": "~10.0.4",
        "@ctrl/ngx-codemirror": "^4.0.0",
        "codemirror": "^5.57.0",
        "n3": "^1.6.2",
        "rxjs": "~6.5.5",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1000.3",
        "@angular/cli": "~10.0.3",
        "@angular/compiler-cli": "~10.0.4",
        "@compodoc/compodoc": "^1.1.11",
        "@types/codemirror": "^0.0.97",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/n3": "^1.4.4",
        "@types/node": "^14.0.27",
        "@types/rdf-js": "^4.0.0",
        "angular-cli-ghpages": "^0.6.2",
        "codelyzer": "^6.0.0",
        "cross-var-no-babel": "^1.2.0",
        "gzipper": "^4.0.2",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~3.3.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.1.1",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "tslint-angular": "^3.0.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "~3.9.5"
    }
}
